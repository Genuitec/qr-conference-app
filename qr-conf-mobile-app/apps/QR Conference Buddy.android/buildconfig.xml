<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration
    xmlns="http://schemas.genuitec.com/myeclipse/mobile"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schemas.genuitec.com/myeclipse/mobile http://schemas.genuitec.com/myeclipse/mobile/buildconfig_0_2.xsd"
    version="0.2">
	
	<configId>5592adb99b1e4f3fb101740647c28ee0</configId>
	
	<appId>QR Conference Buddy.android</appId>
	<appName>QR Conference Buddy</appName>
	<packageName>myeclipse.qr_conference_buddy</packageName>
	<outputFileName>qr_conference_buddy.apk</outputFileName>	
	<appVersion>1.0.0</appVersion>
	<appLanguage />
	<author email="" name="" />

	<supportedOrientation>ALL</supportedOrientation>

	<platform>
	    <android>
			<targetDisplaySizes>
			    <item>XL</item>
			    <item>L</item>
			    <item>M</item>
			    <item>S</item>
			</targetDisplaySizes>    
			<permissions>
			    <item>NETWORK</item>
			</permissions>		   
	    </android>
	</platform>

	<sourceFolders>
		<item exclusionPatterns="" inclusionPatterns="www/" />
	</sourceFolders>
	<startupURI>index.html</startupURI>

	<brandingImages>
		<app-image image-path="apps/QR Conference Buddy.android/images/36.png"
			image-size="36x36" image-type="ICON" />
		<app-image image-path="apps/QR Conference Buddy.android/images/48.png"
			image-size="48x48" image-type="ICON" />
		<app-image image-path="apps/QR Conference Buddy.android/images/72.png"
			image-size="72x72" image-type="ICON" />
		<app-image image-path="apps/QR Conference Buddy.android/images/96.png"
			image-size="96x96" image-type="ICON" />
	</brandingImages>

</configuration>
