<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration
    xmlns="http://schemas.genuitec.com/myeclipse/mobile"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schemas.genuitec.com/myeclipse/mobile http://schemas.genuitec.com/myeclipse/mobile/buildconfig_0_2.xsd"
    version="0.2">

	<configId>e323a6838dab4f7e93019a66f60dd1c6</configId>	
	
	<appId>QR Conference Buddy.ios</appId>
	<appName>QR Conference Buddy</appName>
	<packageName>QR Conference Buddy</packageName>
	<outputFileName>qr_conference_buddy.ipa</outputFileName>
	<appVersion>1.0.0</appVersion>
	<appLanguage />
	<author email="" name="" />

	<supportedOrientation>ALL</supportedOrientation>
	
	<platform>
	    <ios>
            <targetDeviceFormFactor>UNIVERSAL</targetDeviceFormFactor>	        
	    </ios>
