		<app-image image-path="apps/QR Conference Buddy.ios/images/iTunesArtwork.png"
			image-size="512x512" image-type="ICON" />
		<app-image image-path="apps/QR Conference Buddy.ios/images/Default.png"
			image-size="320x480" image-type="SPLASH" />
		<app-image image-path="apps/QR Conference Buddy.ios/images/Default@2x.png"
			image-size="640x960" image-type="SPLASH" />
		<app-image image-path="apps/QR Conference Buddy.ios/images/Default-568h@2x.png"
			image-size="640x1136" image-type="SPLASH" />
