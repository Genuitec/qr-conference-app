	<startupURI>index.html</startupURI>

	<brandingImages>
		<app-image image-path="apps/QR Conference Buddy.ios/images/icon.png"
			image-size="57x57" image-type="ICON" />
		<app-image image-path="apps/QR Conference Buddy.ios/images/icon-72.png"
			image-size="72x72" image-type="ICON" />
		<app-image image-path="apps/QR Conference Buddy.ios/images/Default@2x.png"
